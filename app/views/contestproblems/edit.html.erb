<% provide(:title, 'Modifier un problème') %>

<h1><%= title_4((link_to "Concours", contests_path), (link_to "Concours ##{@contest.number}", @contest), (link_to "Problème ##{@contestproblem.number}", @contestproblem), "Modifier") %></h1>

<%= form_for(@contestproblem) do |f| %>
  <%= render 'form', f: f %>
  <%= f.submit "Modifier", class: "btn btn-lg btn-primary" %>
<% end %>
    
<script>
Preview.Init();
Preview.Update();
</script>
